	OPTIONS DEFTYPE = unord;
	TYPESET MorphoclineOrdered = ord: 1 4 7 12 13 14 15 18 25 28 29 30 38 39 41 44 49 58 60 62 65 68 69 71 74 ;

END;

BEGIN PAUP;
	LOG start file = Anseriformes.log;
	SET CRITERION = parsimony;
	Pset msTaxa = uncertain;
	Pset Collapse = No ;
	[DELETE Asteriornis_maastrichtensis ;]
	ASSUME TYPESET = MorphoclineOrdered;
	Outgroup 1 - 2 ;
	Set root = outgroup outRoot = monophyl;
	CONSTRAINTS molecular backbone = ( ( Casuarius_casuarius, Dromaius_novaehollandiae) , ( ( ( Leipoa_ocellata, Alectura_lathami ) , ( Acryllium_vulturinum, ( Callipepla_californica, ( ( Pavo_muticus, ( Gallus_gallus, Coturnix_japonica ) ) , (Perdix_perdix, Lagopus_lagopus ) ) ) ) ) , ( Anhima_cornuta , ( Anseranas_semipalmata , ( Dendrocygna_bicolor , ( Oxyura_australis , ( Cereopsis_novaehollandiae , ( Cygnus_atratus, Anser_caerulescens ) ) , ( Tadorna_tadornoides , ( Chenonetta_jubata , ( Anas_supercilliosa , Aythya_australis , Melanitta_perspicillata  ) ) ) ) ) ) ) ) ) ;
	Set maxtrees = 10000 increase = auto;
	Hsearch constraints = molecular enforce = yes addseq = random nreps = 10000 rstatus = yes nchuck = 100 chuckscore = 1 hold = 10 swap = tbr;
	SAVETREES File = allbest.trees root = yes brlens = yes;
	CONTREE / treefile = STRICTconsensus.tree root = outgroup;
	CONTREE / strict = no majrule = yes treefile = MAJORITY-RULEconsensus.tree root = outgroup;
	DESCRIBETREES / plot = cladogram root = outgroup outroot = monophyl apolist = yes diag = yes opt = acctran;
	BOOTSTRAP nreps=10000 conlevel=50 grpfreq=yes keepall=yes brlens=yes treefile=bootstrap10000.trees replace=yes search=heuristic / addseq=random nreps=100 nchuck=1000 chuckscore=1 hold=10 swap=tbr ;
